<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="430.0" maxWidth="1000.0" minHeight="430.0" minWidth="1000.0" prefHeight="430.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar VBox.vgrow="NEVER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <Menu mnemonicParsing="false" text="Load...">
                          <items>
                            <MenuItem mnemonicParsing="false" onAction="#openSearch" text=".asm file" />
                              <MenuItem mnemonicParsing="false" onAction="#openSearch" text="hex file" />
                          </items>
                        </Menu>
                        <SeparatorMenuItem mnemonicParsing="false" />
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="Settings">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#getPath" text="pic-as path..." />
                    </items>
                  </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </VBox>
   </top>
   <right>
      <HBox BorderPane.alignment="CENTER_LEFT">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <TextArea fx:id="editor" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets left="6.0" right="6.0" />
                     </VBox.margin></TextArea>
                  <HBox VBox.vgrow="NEVER">
                     <children>
                        <Button fx:id="step" mnemonicParsing="false" onAction="#step" text="Step" />
                        <Region HBox.hgrow="ALWAYS" />
                        <Button fx:id="compile" layoutX="70.0" layoutY="15.0" mnemonicParsing="false" onAction="#compile" text="Compile" />
                        <Region layoutX="114.0" layoutY="15.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="run" mnemonicParsing="false" onAction="#run" text="Run" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="60.0" right="60.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </right>
   <left>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <TableView id="table" fx:id="table" HBox.hgrow="ALWAYS">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TableColumn fx:id="addr" minWidth="-1.0" prefWidth="-1.0" text="SFR" />
                  <TableColumn fx:id="val" minWidth="-1.0" prefWidth="-1.0" text="Value" />
               </columns>
            </TableView>
            <TableView id="table2" fx:id="table2" layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TableColumn fx:id="addr2" minWidth="-1.0" prefWidth="-1.0" text="Address" />
                  <TableColumn fx:id="val2" minWidth="-1.0" prefWidth="-1.0" text="Value" />
               </columns>
            </TableView>
         </children>
      </HBox>
   </left>
</BorderPane>
